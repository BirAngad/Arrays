#include <iostream>
using namespace std;

int main() {
    int len2;
    cout << "Enter length: ";
    cin >> len2;

    int arrB[50];
    for (int i = 0; i < len2; i++) cin >> arrB[i];

    for (int i = 0; i < len2; i++) {
        for (int j = i + 1; j < len2; j++) {
            if (arrB[i] == arrB[j]) {
                for (int k = j; k < len2 - 1; k++) arrB[k] = arrB[k + 1];
                len2--;
                j--;
            }
        }
    }

    cout << "After removing duplicates: ";
    for (int i = 0; i < len2; i++) cout << arrB[i] << " ";
    cout << endl;

    return 0;
}
